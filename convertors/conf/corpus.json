{	
  "corpus_name": "conlab_besleney",
  "corpus_dir": "../corpus/",
  "meta_filename": "meta.csv",
  "speaker_meta_filename": "meta_speakers.json",
  "meta_fields":
  [
    "filename",
    "speakers",
    "title",
    "year"
  ],
  "languages": ["besleney", "russian"],
  "tier_languages": {
    ".*tx.*": "besleney",
    ".*ft_ru.*": "russian"
  },
  "main_tiers": [".*tx.*"],
  "aligned_tiers": [".*ft_ru.*"],
  "analysis_tiers": {
    ".*_Word.*": "word",
    ".*_Morph.*": "parts",
    ".*_Lemma.*": "lemma",
    ".*_POS.*": "pos",
    ".*_Gloss.*": "gloss",
    ".*_Gramm.*": "gramm"
  },
  "meta_files_ext": true,
  "meta_files_dir": false,
  "meta_files_case_sensitive": true,
  "cg_disambiguate": false,
  "json_indent": 1,
  "convert_quotes": true,
  "gzip": false,
  "left_quot_mark": "«",
  "right_quot_mark": "»",
  "sent_end_punc": "(?:[.!?;;]+(?:[)\\]}>/»]|$)|\\\\n)",
  "sent_start": "^[A-ZÄÖÜА-ЯЁӤӦӴӞӜ]",
  "newline_ends_sent": true,
  "abbreviations": [],
  "glosses": {
    "besleney": [
    ],
    "russian": [
    ]
  }
}
